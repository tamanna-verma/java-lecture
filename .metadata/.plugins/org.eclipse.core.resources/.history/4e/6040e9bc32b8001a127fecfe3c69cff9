package Vehicle;

public class Cars extends vehicle {

	
		int numGears;
		Boolean isConvertible;

	public void printCars() {
		System.out.println("Cars color:"+Colour);
		System.out.println("Cars maxSpeed:"+maxSpeed);
		System.out.println("Cars color:"+numGears);
		System.out.println("Cars isConvertible:"+isConvertible);
	}

}
